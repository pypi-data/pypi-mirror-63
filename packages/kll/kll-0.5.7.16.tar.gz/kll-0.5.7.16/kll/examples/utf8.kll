# Unicode Code Point
# echo -e $(cat utf8.kll | grep U+ | cut -d';' -f'1' | cut -d':' -f'2' | sed 's/U+/\\U/')
U"Q" : U+2227; # ∧
U"W" : U+2228; # ∨
U"E" : U+2229; # ∩
U"R" : U+222A; # ∪
U"T" : U+2282; # ⊂
U"Y" : U+2283; # ⊃
U"U" : U+2200; # ∀
U"I" : U+221E; # ∞
U"O" : U+2203; # ∃
U"P" : U+2202; # ∂

U"A" : U+22A5; # ⊥
U"S" : U+22A4; # ⊤
U"D" : U+22A2; # ⊢
U"F" : U+22A3; # ⊣
U"G" : U+2191; # ↑
U"H" : U+2193; # ↓
U"J" : U+2190; # ←
U"K" : U+2192; # →
U"L" : U+2194; # ↔

U"Z" : U+230A; # ⌊
U"X" : U+2308; # ⌈
U"C" : U+2260; # ≠
U"V" : U+2243; # ≃
U"B" : U+2261; # ≡
U"N" : U+2264; # ≤
U"M" : U+2265; # ≥

U"F1" : U+2160; # Ⅰ
U"F2" : U+2161; # Ⅱ
U"F3" : U+2162; # Ⅲ
U"F4" : U+2163; # Ⅳ

U"Left" : U+261C; # ☜
U"Right" : U+261E; # ☞
U"Up" : U+1F592; # 🖒
U"Down" : U+1F593; # 🖓


# UTF-8 Strings
U"1" : u'This string is unicode!';
U"2" : u'⌨';
U"3" : u'⌨🖮';
U"4" : u'キーボード';
U"5" : u'键盘';
U"6" : u'키보드';

