  <file>
    <body>
      <trans-unit id="hello">
        <source xml:space="preserve">Hello, wonderful world!
</source>
      </trans-unit>
      <trans-unit id="orangutan">
        <source xml:space="preserve">Orangutan has <x id="c" equiv-text="{{count}}"/> banana.
</source>
      </trans-unit>
      <trans-unit id="try">
        <source xml:space="preserve">Try Weblate at &lt;https://demo.weblate.org/&gt;!
</source>
      </trans-unit>
      <trans-unit id="thanks">
        <source><x id="count" equiv-text="{{count}}"/><x id="unit" equiv-text="{{unit}}"/></source>
      </trans-unit>
      <trans-unit id="empty">
        <source></source>
      </trans-unit>
    </body>
  </file>
</xliff>
