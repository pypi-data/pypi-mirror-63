namespace wppapi
{
    struct Rows : public wppapi::Collection /Abstract/
    {
    public:
        virtual HRESULT get_Application(
            Application **Application /Out/) = 0;

        virtual HRESULT get_Parent(
            IDispatch **Parent /Out/) = 0;

        virtual HRESULT Item(
            int Index,
            Row **Item /Out/) = 0;

        virtual HRESULT Add(
            int BeforeRow = 0,
            Row **Add /Out/ = 0) = 0;

    };
};
